{
    "short_name": "Quote Generator",
    "name": "Quote Generator Chrome Extension",
    "description": "A Quote Generator For Google Chrome",
    "version": "0.1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./index.html",
        "default_icon": "images/icon.jpg",
        "width": 800,
        "height": 600
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "icons": {
        "16": "./images/icon16.png",
        "24": "./images/icon24.png",
        "32": "./images/icon32.png",
        "48": "./images/icon48.png",
        "128": "./images/icon128.png",
        "256": "./images/icon256.png"
    }
}
